{
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["jsdoc"],
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:jsdoc/recommended",
    "prettier"
  ],
  "rules": {
    "prefer-arrow-callback": "error",
    "valid-jsdoc": "off"
  }
}
